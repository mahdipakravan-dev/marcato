<div class="alert alert-<%- type %> alert-dismissible fade" id="alert" role="alert">
  <strong><%- text %></strong>
  <% if(closeBtn == true) { %>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  <% } %>
</div>